prototype(Gerdemann.VRView:Scene) < prototype(TYPO3.Neos:Content) {
    identifier = TYPO3.TypoScript:Tag {
        content = ${q(node).property('identifier')}
        @process.editable = ContentElementEditable {
            property = 'identifier'
        }
    }
    @process.editable = ContentElementWrapping

    backendHotspots  = TYPO3.Neos:ContentCollection {
        nodePath = 'hotspots'
        tagName = 'div'
        @if.inBackend = ${documentNode.context.inBackend}
    }
}